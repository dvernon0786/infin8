# Resend Email Service Configuration
# Copy this file to .env and fill in your actual values

# Resend API Key (get from https://resend.com/api-keys)
# Already configured in Vercel environment variables
RESEND_API_KEY=re_your_api_key_here

# Default Resend email address (will be replaced when domain is verified)
# Example: onboarding@resend.dev
RESEND_FROM_EMAIL=onboarding@resend.dev

# Admin email addresses for notifications (comma-separated)
RESEND_ADMIN_EMAILS=scott@infin8automation.com,engagehubonline@gmail.com
